{
  "name": "simpler-mapper",
  "version": "1.0.0-alpha003",
  "description": "A javascript, profile-based model mapping library.",
  "keywords": [],
  "contributors": [],
  "homepage": "https://github.com/cdibbs/simpler-mapper",
  "engines": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/cdibbs/simpler-mapper"
  },
  "author": {
    "name": "Chris Dibbern",
    "email": "chrisdibbern@gmail.com"
  },
  "bundle": "./dist/bundles/simpler-mapper.es5.js",
  "main": "./dist/index",
  "types": "./dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/cdibbs/simpler-mapper/issues"
  },
  "license": "MIT",
  "scripts": {
    "pretest": "tsc --project tsconfig-test.json",
    "test": "alsatian --tap test/*.spec.js",
    "cover": "nyc npm test",
    "compodoc": "compodoc -p tsconfig.json",
    "e2e": "",
    "build": "npm-run-all tsc webpack",
    "webpack": "webpack --mode production",
    "tsc": "tsc",
    "prepublishOnly": "npm run build",
    "report_coverage": "nyc report --reporter=lcov && codecov"
  },
  "typings": "./dist/index.d.ts",
  "private": false,
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^12.6.8",
    "alsatian": "^3.2.0",
    "alsatian-fluent-assertions": "^1.0.0-beta.5",
    "codecov": "^3.5.0",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "nyc": {
    "include": [
      "test/*.js",
      "test/**/*.js"
    ],
    "extension": [
      ".js"
    ],
    "exclude": [
      "typings",
      "src/e2e/resources",
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.spec.js",
      "**/i-*.js",
      "test/lib/i/index.js"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "require": [
      "source-map-support/register"
    ]
  }
}
